1.
#include <iostream>
#include <vector>

using namespace std;

int main() {
    int N;
    cout << "Введите размер вектора: ";
    cin >> N;

    vector<int> numbers;

    cout << "Введите " << N << " чисел:" << endl;
    for (int i = 0; i < N; ++i) {
        int num;
        cin >> num;
        numbers.push_back(num);
    }

    cout << "Числа в обратном порядке: ";
    for (int i = N - 1; i >= 0; --i) {
        cout << numbers[i] << " ";
    }
    cout << endl;

}
2.
#include <iostream>
#include <vector>
#include <string>

using namespace std;

int main() {
    int N;
    cout << "Введите размер вектора: ";
    cin >> N;
    cin.ignore(); 

    vector<string> strings;

    cout << "Введите " << N << " строк:" << endl;
    for (int i = 0; i < N; ++i) {
        string str;
        getline(cin, str);
        strings.push_back(str);
    }

    cout << "Введите еще одну строку для добавления в вектор: ";
    string additionalString;
    getline(cin, additionalString);
    strings.push_back(additionalString);

    cout << "Содержимое вектора:" << endl;
    for (const auto& str : strings) {
        cout << str << endl;
    }

}
